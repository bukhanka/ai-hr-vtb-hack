{
  "name": "vtbhack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && pnpm db:seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup:env": "cp env.example .env.local",
    "setup:all": "pnpm setup:env && pnpm docker:up && sleep 5 && pnpm db:generate && pnpm db:push && pnpm db:seed",
    "production:build": "docker build -t vtbhack-app .",
    "production:run": "docker run -p 3000:3000 vtbhack-app"
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.14",
    "@livekit/components-styles": "^1.1.6",
    "@prisma/client": "^6.15.0",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "livekit-client": "^2.15.6",
    "livekit-server-sdk": "^2.13.3",
    "next": "15.5.2",
    "pg": "^8.16.3",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
